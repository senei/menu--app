  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "recipient": {
      "type": "string"
    },
    "street": {
      "type": "string"
    },
    "locality": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "postalCode": {
      "type": "string"
    }
  },
  "validations": [],
