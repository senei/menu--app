{
  "name": "user",
  "base": "User",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "phone": {
      "type": "string",
      "comments": "phone number"
    },
    "geopoint": {
      "type": "geopoint",
      "comments": "localisation"
    },
    "wallet": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "address": {
      "type": "belongsTo",
      "model": "address",
      "foreignKey": "addressId"
    },
    "diners": {
      "type": "hasMany",
      "model": "diner",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
